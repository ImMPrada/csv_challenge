{
  "name": "Frontend Structure",
  "description": "General frontend project structure rules",
  "patterns": [
    {
      "pattern": "frontend/src/**/*.{ts,tsx,js,jsx}",
      "action": "move",
      "target": "frontend/src/"
    },
    {
      "pattern": "frontend/public/**/*",
      "action": "move",
      "target": "frontend/public/"
    }
  ],
  "defaultExtensions": {
    "components": ".tsx",
    "pages": ".tsx",
    "utils": ".ts",
    "hooks": ".ts",
    "types": ".ts"
  }
} 